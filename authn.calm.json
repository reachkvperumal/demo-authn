      "costCenter": "CC-1234",
      "compliance": "PCI-DSS",
      "version": "0.1"
    }
  ],
  "controls": {
    "mfa": {
      "description": "Use multi-factor authentication via WebAuthn passkeys (possession + biometric/PIN) to authenticate users.",
      "requirements": [
        {
          "requirement-url": "https://calm.finos.org/release/1.1/controls/mfa",
          "config": {
            "mechanism": "passkey-webauthn",
            "policy": "required",
            "notes": "Passkeys provide phishing-resistant MFA in supported authenticators."
          }
        }
      ]
    },
    "tls": {
      "description": "Encrypt all external and internal HTTP traffic using TLS.",
      "requirements": [
        {
          "requirement-url": "https://calm.finos.org/release/1.1/controls/tls",
          "config": {
            "external": {
              "protocol": "HTTPS"
            },
            "internal": {
              "protocol": "HTTPS"
            },
            "minimumVersion": "TLS1.2"
          }
        }
      ]
    },
    "zero-trust": {
      "description": "Apply zero-trust principles for service-to-service calls (strong identity, least privilege, and continuous verification).",
      "requirements": [
        {
          "requirement-url": "https://calm.finos.org/release/1.1/controls/zero-trust",
          "config": {
            "serviceIdentity": "mTLS",
            "leastPrivilege": true,
            "continuousVerification": true
          }
        }
      ]
    },
    "phishing-resistance": {
      "description": "Ensure authentication is resistant to phishing by using WebAuthn passkeys bound to relying party origins.",
      "requirements": [
        {
          "requirement-url": "https://calm.finos.org/release/1.1/controls/phishing-resistance",
          "config": {
            "mechanism": "webauthn",
            "originBound": true,
            "userVerification": "required"
          }
        }
      ]
    }
  },
